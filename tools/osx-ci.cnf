[mysqld]
socket=/var/run/mysqld/mysqld.sock
ssl-ca=/etc/ssl/certs/mysql/ca-cert.pem
ssl-cert=/etc/ssl/certs/mysql/server-cert.pem
ssl-key=/etc/ssl/certs/mysql/server-key.pem

[client]
socket=/var/run/mysqld/mysqld.sock